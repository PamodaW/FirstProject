<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Qualified_IntakeQuestions_Required</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
ISPICKVAL( Status , &quot;Qualified&quot;),
Intake_Questions_Complete__c  = FALSE
)</errorConditionFormula>
    <errorMessage>Please make sure the Intake Questions checkbox is checked before converting this Lead.</errorMessage>
</ValidationRule>
