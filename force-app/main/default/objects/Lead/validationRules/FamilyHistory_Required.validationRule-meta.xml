<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>FamilyHistory_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
Family_History_of_SUD__c = TRUE,
ISBLANK(Family_History_of_SUD_Notes__c)
)</errorConditionFormula>
    <errorDisplayField>Family_History_of_SUD_Notes__c</errorDisplayField>
    <errorMessage>Please enter the Client&apos;s Family History.</errorMessage>
</ValidationRule>
