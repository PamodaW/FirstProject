<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Qualified_ClinicalClearance_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
ISPICKVAL( Status , &quot;Qualified&quot;),
Clinical_Clearance_Complete__c = FALSE
)</errorConditionFormula>
    <errorMessage>Please make sure Clinical Clearance checkbox is checked before converting this Lead.</errorMessage>
</ValidationRule>
