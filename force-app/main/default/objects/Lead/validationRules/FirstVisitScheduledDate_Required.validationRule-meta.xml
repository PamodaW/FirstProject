<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>FirstVisitScheduledDate_Required</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
ISPICKVAL(Status, &quot;7-First Visit Scheduled&quot;),
ISBLANK(Scheduled_First_Visit_Date__c)
)</errorConditionFormula>
    <errorMessage>Make sure the Scheduled First Visit Date has been completed prior to moving the Lead Status to &quot;First Visit Scheduled&quot;.</errorMessage>
</ValidationRule>
