<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VOB_Completed_Ref_Number</fullName>
    <active>true</active>
    <description>Fields must be filled in for vob to be completed status</description>
    <errorConditionFormula>ISPICKVAL(Status__c, &quot;completed&quot;) &amp;&amp; (ISBLANK( Reference__c ))</errorConditionFormula>
    <errorDisplayField>Reference__c</errorDisplayField>
    <errorMessage>Please enter the reference number for the call</errorMessage>
</ValidationRule>
