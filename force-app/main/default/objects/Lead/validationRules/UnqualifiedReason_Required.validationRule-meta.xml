<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>UnqualifiedReason_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
ISPICKVAL(Status, &quot;9-Unqualified&quot;),
ISPICKVAL(Unqualified_Reason__c , &quot;&quot;)
)</errorConditionFormula>
    <errorDisplayField>Unqualified_Reason__c</errorDisplayField>
    <errorMessage>Please select an Unqualified Reason.</errorMessage>
</ValidationRule>
