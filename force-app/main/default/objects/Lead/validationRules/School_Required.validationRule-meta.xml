<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>School_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
In_School__c  = TRUE,
ISBLANK( School_Info__c  )
)</errorConditionFormula>
    <errorDisplayField>School_Info__c</errorDisplayField>
    <errorMessage>Please enter info about the Client&apos;s school.</errorMessage>
</ValidationRule>
