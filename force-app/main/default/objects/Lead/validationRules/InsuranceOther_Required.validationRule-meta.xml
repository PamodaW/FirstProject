<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>InsuranceOther_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
ISPICKVAL( Insurance_Carrier_Name__c , &quot;Other&quot;),
ISBLANK( Insurance_Carrier_Other__c )
)</errorConditionFormula>
    <errorDisplayField>Insurance_Carrier_Other__c</errorDisplayField>
    <errorMessage>Please enter the Insurance Carrier Other.</errorMessage>
</ValidationRule>
