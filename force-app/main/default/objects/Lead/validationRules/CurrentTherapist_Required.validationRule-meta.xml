<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CurrentTherapist_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
 Current_Therapist__c   = TRUE,
ISBLANK(  Current_Therapist_Info__c   )
)</errorConditionFormula>
    <errorDisplayField>Current_Therapist_Info__c</errorDisplayField>
    <errorMessage>Please enter info about the Client&apos;s Current Therapist.</errorMessage>
</ValidationRule>
