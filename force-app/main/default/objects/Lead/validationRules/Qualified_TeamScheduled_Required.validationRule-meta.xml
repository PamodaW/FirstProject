<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Qualified_TeamScheduled_Required</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
ISPICKVAL( Status , &quot;Qualified&quot;),
Team_Scheduled__c = FALSE
)</errorConditionFormula>
    <errorMessage>Please make sure the Team Scheduled checkbox is checked before converting this Lead.</errorMessage>
</ValidationRule>
