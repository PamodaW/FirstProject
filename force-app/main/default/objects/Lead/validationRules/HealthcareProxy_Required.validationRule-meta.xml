<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>HealthcareProxy_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
Healthcare_Proxy_in_Place__c = TRUE,
ISBLANK(Healthcare_Proxy_Info__c)
)</errorConditionFormula>
    <errorDisplayField>Healthcare_Proxy_Info__c</errorDisplayField>
    <errorMessage>Please enter info about the Client&apos;s Healthcare Proxy.</errorMessage>
</ValidationRule>
