<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>HistoryofTrauma_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
History_of_Trauma__c = TRUE,
ISBLANK(History_of_Trauma_Info__c)
)</errorConditionFormula>
    <errorDisplayField>History_of_Trauma_Info__c</errorDisplayField>
    <errorMessage>Please enter info about the Client&apos;s History of Trauma.</errorMessage>
</ValidationRule>
