<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ASAMCompletedBy_Required</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
  ISPICKVAL(Status, &quot;4-ASAM Completed&quot;),
ISBLANK( ASAM_Completed_By__c)
)</errorConditionFormula>
    <errorDisplayField>ASAM_Completed_By__c</errorDisplayField>
    <errorMessage>Please enter the name of the person who completed the ASAM.</errorMessage>
</ValidationRule>
