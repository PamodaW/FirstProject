<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>InsuranceMemberID_Required</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
NOT(ISPICKVAL(Insurance_Carrier_Name__c , &quot;&quot;)),
ISBLANK(Insurance_Member_ID__c)
)</errorConditionFormula>
    <errorDisplayField>Insurance_Member_ID__c</errorDisplayField>
    <errorMessage>Please enter the Client&apos;s Insurance Member ID.</errorMessage>
</ValidationRule>
